    "@tailwindcss/typography": "^0.5.7",
    "date-fns": "^2.29.3",
    "next": "^12.3.1",
    "next-sanity": "^0.8.4",
    "next-sanity-image": "^4.1.0",
    "next-seo": "^5.5.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
