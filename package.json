{
  "private": true,
  "scripts": {
    "dev": "pnpm lint && next dev",
    "prebuild": "echo 'Building Sanity to public/studio' && cd studio && pnpm i && npx @sanity/cli build ../../public/studio -y && echo 'Done'",
    "build": "next build",
    "nextbuild": "next build && next export",
    "start": "next start ",
    "sanity": "cd studio && sanity start",
    "export": "cd studio && sanity dataset export production ../.sanity-template/data/production.tar.gz",
    "lint": "next lint --fix",
    "deploy:prod": "pm2 startOrRestart ecosystem.config.js --env prod"
  },
  "dependencies": {
